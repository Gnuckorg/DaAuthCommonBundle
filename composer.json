{
    "name": "da/auth-common-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2's bundle sharing commons for DaOAuth* and DaApi* bundles",
    "keywords": ["auth", "oauth", "api", "common", "bundle", "da"],
    "homepage": "https://github.com/Gnuckorg/DaAuthCommonBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Prelot",
            "email": "tprelot@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/dependency-injection" : "~2.1",
        "symfony/config" : "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Da\\AuthCommonBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Da/AuthCommonBundle"
}